{
  "name": "QADEER-AI",
  "version": "5.0.0",
  "description": "Ultra Fast WhatsApp Bot - QADEER AI",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "keywords": [
    "qadeer-ai",
    "whatsapp-bot",
    "ai-bot",
    "fast-bot",
    "multi-device",
    "baileys-md",
    "whatsapp-automation",
    "advanced-bot",
    "qadeer-bot",
    "urxd-bot"
  ],
  "scripts": {
    "start": "node index.js",
    "dev": "node --watch index.js",
    "pm2-start": "pm2 start index.js --name QADEER-AI --node-args='--max-old-space-size=8192'",
