  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "pm2": "pm2 start index.js --node-args='--max-old-space-size=4096' --exp-backoff-restart-delay=100 --name QADEER-AI",
    "pm2:cluster": "pm2 start index.js -i 2 --name QADEER-AI-CLUSTER",
    "stop": "pm2 stop QADEER-AI",
    "restart": "pm2 restart QADEER-AI",
    "logs": "pm2 logs QADEER-AI",
    "monitor": "pm2 monit",
    "clean": "pm2 delete QADEER-AI && pm2 save --force",
    "test": "node --test",
    "format": "prettier --write ."
